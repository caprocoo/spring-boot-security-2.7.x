CREATE TABLE CAPROCOO.OB_ACCOUNT (
	ACCOUNT_ID VARCHAR2(50),
	ACCOUNT_NAME VARCHAR2(50),
	TEL_NO_FIRST CHAR(3),
	TEL_NO_SECOND CHAR(4),
	TEL_NO_THIRD CHAR(4),
	ACCOUNT_EMAIL VARCHAR2(100),
	ACCOUNT_PWD VARCHAR2(50)
);

ALTER TABLE CAPROCOO.OB_ACCOUNT
ADD CONSTRAINT OB_ACCOUNT_PK PRIMARY KEY (ACCOUNT_ID)
ENABLE;

ALTER TABLE CAPROCOO.OB_ACCOUNT MODIFY ACCOUNT_PWD VARCHAR2(50) NOT NULL;

ALTER TABLE CAPROCOO.OB_ACCOUNT ADD ACTIVATED CHAR(1);
